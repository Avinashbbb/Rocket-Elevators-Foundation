<!DOCTYPE html>
<html>
  <head>
    <title>RocketElevatorsInformationSystem</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!--WHAT IS: async, refer -->
    <script async src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCpty8Aa0z6JF49Xjjxx9XWa2IiYGFja9s&callback=mapsLoaded&libraries=places" async refer></script>

    <script> var plugin_path = 'assets/plugins/' </script>
  </head>

  <body>
  
   <%= render 'partials/header' %>

    <%= yield %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= render 'partials/footer' %>
    <%= yield :after_js %>
  </body>
</html>